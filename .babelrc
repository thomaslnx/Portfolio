{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["styled-jsx/babel", { "optimizedForSpeed": true}],
    [
      "module-resolver", {
        "root": ["./src"],
        "extensions": [".ts", ".tsx", ".js"]
      }
    ],
    ["styled-components", {
      "ssr": true,
      "displayName": true
    }]
  ]
}
